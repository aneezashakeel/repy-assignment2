# Test: Multiple writes ending invalid should not save
try:
  myfile = ABopenfile("testfile11.txt", True)
  myfile.writeat("SAFE", 0)
  myfile.writeat("R", 4)  # Makes it invalid
  myfile.close()
  
  myfile = ABopenfile("testfile11.txt", False)
  data = myfile.readat(None, 0)
  if "SAFER" in data:
    log("BUG: Invalid multi-write was saved!")
  myfile.close()
except:
  pass
