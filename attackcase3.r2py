# Test: Data starting with space should not be allowed  
try:
  myfile = ABopenfile("testfile3.txt", True)
  myfile.writeat("S", 0)
  myfile.writeat(" DATAE", 1)  # Starts with space
  myfile.close()
  
  myfile = ABopenfile("testfile3.txt", False)
  data = myfile.readat(None, 0)
  if " " in data:
    log("BUG: Space-prefixed data was allowed!")
  myfile.close()
except:
  pass
