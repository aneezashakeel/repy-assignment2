# Attack: Write data starting with space
if "testfile3.txt.a" in listfiles():
  removefile("testfile3.txt.a")
if "testfile3.txt.b" in listfiles():
  removefile("testfile3.txt.b")

myfile = ABopenfile("testfile3.txt", True)

try:
  myfile.writeat(" SAFE", 0)  # Starts with space
  myfile.close()
  
  myfile = ABopenfile("testfile3.txt", False)
  data = myfile.readat(None, 0)
  if data[0] == " ":
    log("Data starting with space was allowed!")
  myfile.close()
except:
  pass
