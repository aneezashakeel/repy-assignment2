# Attack: Only write 'S' without 'E'
if "testfile8.txt.a" in listfiles():
  removefile("testfile8.txt.a")
if "testfile8.txt.b" in listfiles():
  removefile("testfile8.txt.b")

myfile = ABopenfile("testfile8.txt", True)

try:
  myfile.writeat("S", 0)  # Only S, no E
  myfile.close()
  
  myfile = ABopenfile("testfile8.txt", False)
  data = myfile.readat(None, 0)
  if data == "S":
    log("File with only S was saved!")
  myfile.close()
except:
  pass
