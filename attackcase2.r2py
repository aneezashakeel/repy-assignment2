# Attack: Write invalid data (no 'E' at end)
if "testfile2.txt.a" in listfiles():
  removefile("testfile2.txt.a")
if "testfile2.txt.b" in listfiles():
  removefile("testfile2.txt.b")

myfile = ABopenfile("testfile2.txt", True)

try:
  myfile.writeat("SDATA", 0)  # Doesn't end with 'E'
  myfile.close()
  
  myfile = ABopenfile("testfile2.txt", False)
  data = myfile.readat(None, 0)
  if "SDATA" in data:
    log("Invalid data without E was saved!")
  myfile.close()
except:
  pass
